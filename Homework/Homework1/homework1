      #inlcude<stdio.h>

      /*
      *Prints the function name
      *Then all arguments on a
      *new line
      */ 

      int main(int argc, char *argv[])
      {
          int i=0;

          for (i = 0; i < argc; i++) {
              printf("%s\n", argv[i]);
          }
          return 0;
      }

